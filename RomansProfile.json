{
  "title": "Roman's modifiers",
  "rules": [
    {
      "description": "Change Fn+Enter&Space to Command+Enter&Space,Change Command+Shift to Control+Space,Change Option+Tab to Command+Tab.",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_shift",
            "modifiers": {
              "mandatory": [
                "left_option"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "spacebar",
              "modifiers": [
                "left_control"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {
              "mandatory": [
                "fn"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter",
              "modifiers": [
                "left_command"
              ]
            }
          ]
